<!DOCTYPE html>
<html lang="en">
<head>
	<title>Admin Login</title>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
    
<!--Using  BootstrapCSS-->
	<link rel="stylesheet" type="text/css" href="/assets/styles/bootstrap.min.css">
<!--Using Fonts-->
	<link rel="stylesheet" type="text/css" href="/assets/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
<!--Using Icon Font -->
    <link rel="stylesheet" type="text/css" href="/assets/fonts/Linearicons-Free-v1.0.0/icon-font.min.css">
    
<!--Using Animate CSS-->
	<link rel="stylesheet" type="text/css" href="/assets/styles/animate.css">


<!--Using HAmburgers CSS-->	
	<link rel="stylesheet" type="text/css" href="/assets/styles/hamburgers.min.css">

<!--Using Select CSS-->
	<link rel="stylesheet" type="text/css" href="/assets/styles/select2.min.css">



<!--Using Util css  and main css-->
	<link rel="stylesheet" type="text/css" href="/assets/styles/util.css">
	<link rel="stylesheet" type="text/css" href="/assets/styles/main.css">

<!-- Start of the Login -->
</head>
<body>
	
	<div class="limiter">
		<div class="container-admin100">
			<div class="wrap-admin100 p-l-50 p-r-50 p-t-77 p-b-30">
				<form class="admin100-form validate-form">
					<span class="admin100-form-title p-b-55">
						Admin Login
                    </span>
                    

					<!-- Used a Validation-->
					<div class="wrap-input100 validate-input m-b-16" data-validate = "Valid email is required: ex@abc.xyz">
						<input class="input100" type="text" name="email" placeholder="Email">
						<span class="focus-input100"></span>
						<span class="symbol-input100">
							<span class="lnr lnr-envelope"></span>
						</span>
					</div>
					<!--Password Validation-->
					<div class="wrap-input100 validate-input m-b-16" data-validate = "Password is required">
						<input class="input100" type="password" name="pass" placeholder="Password">
						<span class="focus-input100"></span>
						<span class="symbol-input100">
							<span class="lnr lnr-lock"></span>
						</span>
					</div>
                    <!--Used a checkbox-->
					<div class="container-admin100-form-btn p-t-25">
						<button class="admin100-form-btn">
							Login
						</button>&nbsp;
					</div>
					<a class="txt1 bo1 hov1" href="#">
						&nbsp;Forgot Password ?						
					</a>
					<a class="txt1 bo1 hov1" href="/">
						&nbsp;HOME						
					</a>
					</div>
				</form>
			</div>
		</div>
	</div>
	
    <!--Using JQuery Scripts-->	

	<script src="/assets/src/jquery-3.2.1.min.js"></script>


    <!--Using bootstrap Scripts-->

        <script src="/assets/src/popper.js"></script>
        <script src="/assets/src/bootstrap.min.js"></script>


    <!--Using Select Scripts-->

        <script src="/assets/src/select2.min.js"></script>


    <!--Using Main Javascript-->

        <script src="/assets/src/main.js"></script>

</body>
</html>