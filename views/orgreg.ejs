<!DOCTYPE html>
<html lang="en">
<head>
	<title>Register</title>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
<!-- Icon image using -->	
	<link rel="icon" type="image/png" href="images/icons/favicon.ico"/>
<!--Using  BootstrapCSS-->
<link rel="stylesheet" type="text/css" href="/assets/styles/bootstrap.min.css">
<!--Using Fonts-->
	<link rel="stylesheet" type="text/css" href="/assets/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
<!--Using Icon Font -->
    <link rel="stylesheet" type="text/css" href="/assets/fonts/Linearicons-Free-v1.0.0/icon-font.min.css">
    
<!--Using Animate CSS-->
	<link rel="stylesheet" type="text/css" href="/assets/styles/animate.css">


<!--Using HAmburgers CSS-->	
	<link rel="stylesheet" type="text/css" href="/assets/styles/hamburgers.min.css">

<!--Using Select CSS-->
	<link rel="stylesheet" type="text/css" href="/assets/styles/select2.min.css">



<!--Using Util css  and main css-->
	<link rel="stylesheet" type="text/css" href="/assets/styles/util.css">
	<link rel="stylesheet" type="text/css" href="/assets/styles/orgreg.css">
</head>
<!-- Start of Register-->
<body>
	
	<div class="limiter">
		<div class="container-register100">
			<div class="wrap-register100 p-l-50 p-r-50 p-t-77 p-b-30">
				<form class="register100-form validate-form" method="POST" action="/auth/host/signup">
					<span class="register100-form-title p-b-55">
					<h2>Host &nbsp;Register </h2>
					</span>
		
					<!-- Host name as hostname -->
					<div class="wrap-input100 validate-input m-b-16" data-validate ="Host Name Required">
						<input class="input100" type="text" name="hostname" placeholder="Host Name">
						<span class="focus-input100"></span>
						<span class="symbol-input100">
							<i class="fa fa-users" aria-hidden="true"></i>
						</span>
					</div>

					<!-- Streetaddress as street -->
					<div class="wrap-input100 validate-input m-b-16" data-validate = "street Address Required">
						<input class="input100" type="text-area" name="street" placeholder="Street Address ">
						<span class="focus-input100"></span>
						<span class="symbol-input100">
							<i class="fa fa-home" aria-hidden="true"></i>
						</span>
					</div>
						
					<!-- City as city -->
					<div class="wrap-input100 validate-input m-b-16" data-validate = "City Required">
						<input class="input100" type="text" name="city" placeholder="City">
						<span class="focus-input100"></span>
						<span class="symbol-input100">
						<i class="fa fa-building" aria-hidden="true"></i>
						
						</span>

					</div>

					<!-- District as state -->
					<div class="wrap-input100  validate-input m-b-16" data-validate = "District Required">
						<select id="Districts" class="input100" name="state">	
							<option hidden >District</option>
							 <option value="Ampara">Ampara</option>
							 <option value="Anuradhapura">Anuradhapura</option>
							 <option value="Badulla">Badulla</option>
							 <option value="Batticaloa">Batticaloa</option>
							 <option value="Colombo">Colombo</option>
							 <option value="Galle">Galle</option>
							 <option value="Gampaha">Gampaha</option>
							 <option value="Hambantota">Hambantota</option>
							 <option value="Jaffna">Jaffna</option>       
							 <option value="Kalutara">Kalutara</option>
							 <option value="Kandy">Kandy</option>
							 <option value="Kegalle">Kegalle</option>
							 <option value="Kilinochchi">Kilinochchi</option>
							 <option value="Kurunegala">Kurunegala</option>
							 <option value="Mannar">Mannar</option>
							 <option value="Matale">Matale</option>
							 <option value="Matara">Matara</option>
							 <option value="Monaragala">Monaragala</option>
							 <option value="Mulativu">Mulativu</option>
							 <option value="Nuwara Eliya">Nuwara Eliya</option>
							 <option value="Polonnaruwa">Polonnaruwa</option>
							 <option value="Puttalam">Puttalam</option>
							   <option value="Ratnapura">Ratnapura</option>
								<option value="Trincomalee">Trincomalee</option>
								 <option value="Vavuniya">Vavuniya</option>
							</select>

						<span class="focus-input100"></span>
						<span class="symbol-input100">
						</span>
					</div>
					
					<!-- Postal code as postal -->
					<div class="wrap-input100 validate-input m-b-16" data-validate = "Postal code Required">
						<input class="input100" type="text" name="postal" placeholder="Postal code">
						<span class="focus-input100"></span>
						<span class="symbol-input100">
							<i class="fa fa-envelope-open-o" aria-hidden="true"></i>
						</span>
					</div>

					<!-- phone no as contactNo -->
					<div class="wrap-input100 validate-input m-b-16"data-validate = "phone number Required">
						<input class="input100" type="text"  id="tdnumbers" onkeypress="javascript:return isNumber(event)" name="contactNo" placeholder="Contact Number">
						<span class="focus-input100"></span>
						<span class="symbol-input100">
							<i class="fa fa-phone" aria-hidden="true"></i>
						</span>
						<script>
							//  VALIDATION SCRIPT.
							function isNumber(evt) {
								var iKeyCode = (evt.which) ? evt.which : evt.keyCode
								if (iKeyCode != 46 && iKeyCode > 31 && (iKeyCode < 48 || iKeyCode > 57))
									return false;
						
								return true;
							}    
						</script>
					</div>
					<!-- nic as NIC -->
					<div class="wrap-input100 validate-input m-b-16" data-validate = "NIC Required">
						<input class="input100" type="text" name="NIC" placeholder="National Identity No">
						<span class="focus-input100"></span>
						<span class="symbol-input100">
							<i class="fa fa-id-card-o" aria-hidden="true"></i>
						</span>
					</div>
					
					<!-- dob as DOB -->
					<div class="wrap-input100 validate-input m-b-16" data-validate = "date required">
						<label>Date of Birth :&nbsp;</label>
						<br>
						<i class="fa fa-birthday-cake" aria-hidden="true"></i>
						<input class="input50" type="date" required name="DOB"  placeholder="Date of Birth ">
						<span class="focus-input100"></span>
						<span class="symbol-input100">
					
						</span>
					</div>
					<!-- Email as email -->
					<div class="wrap-input100 validate-input m-b-16" data-validate = "Valid email is required: ex@abc.xyz">
						<input class="input100" type="email" required name="email" placeholder="E-mail">
						<span class="focus-input100"></span>
						<span class="symbol-input100">
							<i class="fa fa-envelope-o" aria-hidden="true"></i>
						</span>
					</div>
					<!-- Country as country -->
					<div class="wrap-input100 validate-input m-b-16" data-validate = "Country">
						<input class="input100" type="country" name="country" placeholder="Country ">
						<span class="focus-input100"></span>
						<span class="symbol-input100">
							<i class="fa fa-globe" aria-hidden="true"></i>
						</span>
					</div>

					<!--Password Validation-->
					<div class="wrap-input100 validate-input m-b-16" data-validate = "Password is required">
						<input class="input100" type="password" minlength="6" required name="password" placeholder="Password">
						<span class="focus-input100"></span>
						<span class="symbol-input100">
							<i class="fa fa-lock" aria-hidden="true"></i>
						</span>
					</div>

					<!--Password Validation-->
					<div  class="wrap-input100 validate-input m-b-16" data-validate = "Password is required">
						<input class="input100" type="password" required minlength="6" name="pass" placeholder="Confirm Password">
						<span class="focus-input100"></span>
						<span class="symbol-input100">
							<i class="fa fa-lock" aria-hidden="true"></i>
						</span>
					</div>

					
					
					<div class="container-register100-form-btn p-t-25">
						<button class="register100-form-btn">
							Register
						</button>
					</div>
					
					<div class="text-center w-full p-t-115">
						<span class="txt1">
							<h3>Already a member?</h3>
						</span>
						<a class="txt1 bo1 hov1" hre66f="#">
							<h3>Login now</h3>							
						</a>
					</div>
				</form>
				<div class="container">
					<div class="row">
						<div style="margin-left:40%">
						<a href="/home" class="mt-3 btn-home m-b-10  btn btn-lg btn-primary">
							<i class="fa fa-home"></i>
							Home
						</a></div>
					</div>
				</div>
			</div>
		</div>
	</div>
	

<!--Using JQuery Scripts-->	

<script src="/src/jquery-3.2.1.min.js"></script>


<!--Using bootstrap Scripts-->

	<script src="/src/popper.js"></script>
	<script src="/src/bootstrap.min.js"></script>


<!--Using Select Scripts-->

	<script src="/src/select2.min.js"></script>


<!--Using Main Javascript-->

	<script src="/src/main.js"></script>
	

	

</body>
</html>